<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ezartech.demo.pokemongoexperiment" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PokemonGOXP</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="info@ezartech.com" href="http://ezartech.com">
        Wayne Parrott
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="android">
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <plugin name="cordova-plugin-device" spec="~1.1.2" />
    <plugin name="cordova-plugin-media"  spec="~2.3.0" />
    <plugin name="com.ezartech.ezar.videooverlay" spec="/Users/wayne/dev/ezar/plugins/com.ezartech.ezar.videoOverlay" />
    <plugin name="com.ezartech.ezar.snapshot"     spec="/Users/wayne/dev/ezar/plugins/com.ezartech.ezar.snapshot" />
    
</widget>
